<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF 
  xmlns:adms="http://www.w3.org/ns/adms#" 
  xmlns:cnt="http://www.w3.org/2008/content#" 
  xmlns:dcat="http://www.w3.org/ns/dcat#" 
  xmlns:dct="http://purl.org/dc/terms/" 
  xmlns:eposap="http://www.epos-ip.org/" 
  xmlns:foaf="http://xmlns.com/foaf/0.1/" 
  xmlns:http="http://www.w3.org/2006/http#" 
  xmlns:locn="http://www.w3.org/ns/locn#" 
  xmlns:org="http://www.w3.org/ns/org#" 
  xmlns:owl="http://www.w3.org/2002/07/owl#" 
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
  xmlns:schema="http://schema.org/" 
  xmlns:skos="http://www.w3.org/2004/02/skos/core#" 
  xmlns:vcard="http://www.w3.org/2006/vcard/ns#" 
>
  
  <!-- EPOS Catalog -->
  <dcat:Catalog rdf:about="[catalogue uri]">
    <dct:title xml:lang="en">Example of catalog</dct:title>
    <dct:description xml:lang="en">Some description</dct:description>
<!-- 
  @andrea: For publishers / organisations it is preferable to have a URI denoting them.
  This can be a URI from an official register (or a register you may be 
  maintaining in your project). Otherwise, if not available, use the URL
  of their Web site.
  
  BTW, by using the URI, you can keep the description of the publisher separated
  from the catalog record, and it can be specified only once. Which is good for
  maintenance purposes. I.e.:
  
  <dcat:Catalog rdf:about="">
    ...
     <dct:publisher rdf:about="[organization uri]">
    ...
  </dcat:Catalog>
 
  <foaf:Organization rdf:about="[organisation uri]">
    <foaf:name>publisher name</foaf:name>      
  </foaf:Organization>

-->    
    <dct:publisher>
      <foaf:Organization rdf:about="[organisation uri]">
        <foaf:name>publisher name</foaf:name>      
      </foaf:Organization>
    </dct:publisher>
    
    <dcat:dataset>
<!--
  @andrea: Datasets must have a (working) HTTP URI.
-->      
      <dcat:Dataset rdf:about="[http://.../]GFZ.4.1.2016.005">
        <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GFZ.4.1.2016.005</dct:identifier> <!-- Unique Resource Identifier -->
        <dct:title xml:lang="en">My Dataset</dct:title><!-- Title -->
        <dct:description xml:lang="en">This dataset describes...</dct:description><!-- Abstract -->
        <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</dct:issued><!-- Date of Publication -->
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</dct:modified><!-- Date of Revision -->
<!-- @andrea: You can use the relevant codelist maintained by the EU Publications office -->    
        <dct:language rdf:resource="http://publications.europa.eu/resource/authority/language/ENG"/><!-- Language -->
<!--        
        <dct:language>
          <dct:LinguisticSystem>eng</dct:LinguisticSystem>
        </dct:language>
-->        
        <dct:provenance><!-- Lineage -->
          <dct:ProvenanceStatement>
            <rdfs:label xml:lang="en">GFZ provenance</rdfs:label>
          </dct:ProvenanceStatement>
        </dct:provenance>
<!-- @andrea: You can use the relevant codelist maintained by the INSPIRE Registry -->    
        <dct:type rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/ResourceType/dataset"/><!-- Resource type -->
<!--        
        <dct:type><
          <skos:Concept>
            <skos:prefLabel>Resource type</skos:prefLabel>
          </skos:Concept>
        </dct:type>
-->        
        <dcat:keyword>keyword0</dcat:keyword><!-- Keywords (key-phrase) -->
        <dcat:keyword>keyword1</dcat:keyword><!-- Keywords (key-phrase) -->
        <dcat:keyword>keyword2</dcat:keyword><!-- Keywords (key-phrase) -->
        <dcat:keyword>keywordN</dcat:keyword><!-- Keywords (key-phrase) -->
<!-- @andrea: Whenever possible, you should use URIs for dcat:theme, dct:subject -->        
        <dcat:theme> <!-- Domain -->
          <skos:Concept>
            <skos:prefLabel xml:lang="en">Some domain</skos:prefLabel>
          </skos:Concept>
        </dcat:theme>
<!-- @andrea: To be compliant with DCAT-AP you should specify at least one of the MDR Data Themes -->        
        <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/[theme]"/> <!-- Domain -->
<!-- 
     @andrea: There a codelist for this, so you can check if it is suitable to you. The description is here: 
     
     http://publications.europa.eu/mdr/resource/authority/access-right/html/access-right-eng.html
     
     The base URI is:
     
     http://publications.europa.eu/resource/authority/access-right/
     
-->        
        <dct:accessRights><!-- Public Access Limitations -->
          <dct:RightsStatement>
            <rdfs:label xml:lang="en">Some access Rights</rdfs:label>
          </dct:RightsStatement>
        </dct:accessRights>
<!-- @andrea: the CRS should be specified with the relevant URI from the OGC EPSG register -->        
        <dct:conformsTo rdf:resource="http://www.opengis.net/def/crs/EPSG/0/4326"/> <!-- Spatial Reference System -->
        <dcat:landingPage rdf:resource="http://tempuri.org"/>
        <dct:spatial><!-- Spatial coordinates -->
          <dct:Location>
            <locn:geometry rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">POLYGON(-10.58 70.09,34.59 70.09,34.59 34.56,-10.58 34.56, -10.58 70.09)</locn:geometry>
          </dct:Location>
        </dct:spatial>
        <dct:temporal><!-- Temporal extent -->
          <dct:PeriodOfTime>
            <schema:startDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</schema:startDate>
            <schema:endDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</schema:endDate>
          </dct:PeriodOfTime>
        </dct:temporal>
        
        <dcat:distribution>
          <dcat:Distribution>
            <dct:title xml:lang="en">Title of distribution</dct:title>
            <dct:description xml:lang="en">Some description</dct:description>
            <dcat:accessURL rdf:resource="[access URL]"/>
            <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</dct:issued>
            <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</dct:modified>
<!-- @andrea: You can use the relevant codelist maintained by the EU Publications office -->    
            <dct:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/[format]"/><!-- Distribution format -->
<!--            
            <dct:format>
              <dct:MediaTypeOrExtent>Distribution format</dct:MediaTypeOrExtent>
            </dct:format>          
-->            
<!-- 
  @andrea: Whenever possible, you should use URIs for licences. 
  
  A register of standard licences is available at:
  
  http://publications.europa.eu/mdr/resource/authority/licence/html/licences-eng.html
-->
            <dct:license><!-- Access and use description -->
              <dct:LicenseDocument>
                <dct:type>
                  <skos:Concept>
                    <skos:prefLabel>Distribution license</skos:prefLabel>
                  </skos:Concept>
                </dct:type>
              </dct:LicenseDocument>
            </dct:license>
            <foaf:page rdf:resource="http://tempuri.org"/>
          </dcat:Distribution>
        </dcat:distribution>
        
        <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</dct:created> <!-- Data creation -->
<!-- @andrea: You can use the relevant codelist maintained by the INSPIRE Registry -->    
        <dct:subject rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/TopicCategory/[category]"/><!-- Topic Category -->
<!--        
        <dct:subject>
          <skos:Concept>
            <skos:prefLabel>EARTH SCIENCE</skos:prefLabel>
          </skos:Concept>
        </dct:subject>
-->        
        <cnt:characterEncoding rdf:datatype="http://www.w3.org/2001/XMLSchema#string">UTF-8</cnt:characterEncoding> <!-- Characterset -->
        <dcat:contactPoint rdf:resource="[http://.../]personID01"/> <!--- Point of Contact -->
        <eposap:responsibleParty rdf:resource="[http://.../]personID01"/> <!---Responsible Party -->
        <rdf:comment xml:lang="en">Some Spatial Resolution</rdf:comment><!-- Spatial Resolution -->
<!-- @andrea: There's a code list here, yet to be added to the INSPIRE Registry -->    
        <adms:representationTechnique rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/SpatialRepresentationTypeCode/vector"/><!-- Spatial Rapresentation Type -->
<!--        
        <adms:representationTechnique>
          <skos:Concept>
            <skos:prefLabel>Vector</skos:prefLabel>
          </skos:Concept>
        </adms:representationTechnique>
-->        
      </dcat:Dataset>
    </dcat:dataset>

    <!-- Attribute specific to the metadata -->
    <dcat:record>
      <dcat:CatalogRecord rdf:about="[http://.../]idvalue3"><!-- ATTRIBUTE SPECIFIC TO METADATA -->
        <foaf:primaryTopic rdf:resource="[dataset uri]"/> <!-- Topic -->
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</dct:modified> <!-- Updated -->
<!-- @andrea: You can use the relevant codelist maintained by the EU Publications office -->    
        <dct:language rdf:resource="http://publications.europa.eu/resource/authority/language/ENG"/><!-- Language -->
<!--
        <dct:language>        
          <dct:LinguisticSystem>eng</dct:LinguisticSystem>
        </dct:language>
-->        
        <dct:title xml:lang="en">Record name</dct:title><!-- Standard Name -->
        <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">idvalue3</dct:identifier><!-- File identifier -->
        <owl:versionInfo xml:lang="en">Record version</owl:versionInfo><!-- Standard Version -->
        <cnt:characterEncoding rdf:datatype="http://www.w3.org/2001/XMLSchema#string">UTF-8</cnt:characterEncoding><!-- Characterset -->
        <dcat:contactPoint rdf:resource="[http://.../]personID01"/><!-- Point of contact -->
        <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</dct:created> <!-- Created -->
      </dcat:CatalogRecord>
    </dcat:record>
  </dcat:Catalog>

  <!-- EPOS Person -->
<!-- @andrea: You can model this entry by using both FOAF and vCard, so you can use it for different roles (contact point, publisher, etc. -->  
  <vcard:Individual>
    <vcard:fn>Ritter, Malte Christian</vcard:fn><!-- Last Name, Given Name -->
    <vcard:hasAddress><!-- Postal Address -->
      <vcard:Address>
        <vcard:street-address>Street Address</vcard:street-address>
        <vcard:locality>Locality</vcard:locality>
        <vcard:postal-code>Postal-code</vcard:postal-code>
        <vcard:country-name>Country-name</vcard:country-name>
      </vcard:Address>
    </vcard:hasAddress>
    <vcard:hasEmail rdf:resource="mailto:ritter@test.org"/><!-- Email -->
    <vcard:hasTelephone rdf:resource="tel:+00434585622"/><!-- Phone -->
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">personID01</dct:identifier> <!-- Unique Identifier -->
<!-- @andrea: You can use the W3C Organization Ontology to model this information -->    
    <org:memberOf rdf:resource="[http://.../]organisationID01"/> <!-- Organisation -->
<!--    
    <eposap:affiliation rdf:resource="[http://.../]organisationID01"/>
-->    
<!-- @andrea: You can use the relevant codelist maintained by the EU Publications office -->    
    <dct:language rdf:resource="http://publications.europa.eu/resource/authority/language/ENG"/><!-- Language for contact -->
<!--    
    <dct:language>
      <dct:LinguisticSystem>Eng</dct:LinguisticSystem>
    </dct:language>
-->    
    <schema:qualifications>Qualification</schema:qualifications><!-- Qualification -->
    <eposap:hasCV rdf:resource="http://test.org/cv"/><!-- CV -->
  </vcard:Individual>

  <!-- EPOS Organisation -->
<!-- @andrea: You can model this entry by using both FOAF and vCard, so you can use it for different roles (contact point, publisher, etc. -->  
  <rdf:Description rdf:about="[http://.../]organisationID01">
    <rdf:type rdf:resource=""/>
    <rdf:type rdf:resource=""/>
<!-- @andrea: You should use attribute @xml:lang to specify the language for the name, and not different "fields" / properties -->  
    <vcard:fn xml:lang="en">GFZ German Research Center for Geosciences</vcard:fn><!-- Title (english) -->
    <vcard:fn xml:lang="de">GFZ German Research Center for Geosciences</vcard:fn><!-- Title (native lang) -->
    <vcard:hasAddress><!-- Postal Address -->
      <vcard:Address>
        <vcard:street-address>Street Address</vcard:street-address>
        <vcard:locality>Locality</vcard:locality>
        <vcard:postal-code>Postal-code</vcard:postal-code>
        <vcard:country-name>Country-name</vcard:country-name>
      </vcard:Address>
    </vcard:hasAddress>
    <vcard:hasEmail rdf:resource="mailto:gfz@test.org"/><!-- Email -->
    <vcard:hasURL rdf:resource="http://www.gfz-potsdam.de/en/home/"/><!-- Website -->
    <vcard:hasLogo rdf:resource="http://test.org/logo.png"/><!-- Logo -->
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">organisationID01</dct:identifier><!-- Unique Identifier -->
    <eposap:scientificContact rdf:resource="[http://.../]personID01"/><!-- Scientific Contact -->
<!-- @andrea: to specify where it is located you should use locn:location -->    
    <locn:location><!-- Spatial Coordinates -->
      <dct:Location>
        <locn:geometry rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">POINT(9.1927945677761, 45.463925903103,0)</locn:geometry>
      </dct:Location>
    </locn:location>
<!-- @andrea: Not sure which are the possible "types" of organisation you support. There are however codelists for this -->    
    <dct:type rdf:resource="[organisation type uri]"/><!-- Type -->
<!--    
    <dct:type>
      <skos:Concept>
        <skos:prefLabel>Organisation Type</skos:prefLabel>
      </skos:Concept>
    </dct:type>
-->    
    <eposap:legalContact rdf:resource="[http://.../]personID01"/><!-- Legal Contact -->
    <eposap:financialContact rdf:resource="[http://.../]personID01"/><!-- Financial Contact -->
<!-- @andrea: You can use the W3C Organization Ontology to model this information -->    
    <org:subOrganizationOf rdf:resource="[http://.../]organisationID01"/><!-- Is part of -->
<!--    
    <eposap:isPartOf rdf:resource="[http://.../]organisationID01"/>
-->    
    <eposap:associatedProjects rdf:resource="[http://.../]projectID01"/><!-- Associated Project -->
  </rdf:Description>

  <!-- EPOS Project -->
  <foaf:Project rdf:about="[http://.../]projectID01">
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">projectID01</dct:identifier>
    <dct:title xml:lang="en">Project name</dct:title><!-- Name (Title) -->
    <dct:description xml:lang="en">Some description</dct:description><!-- Description (Abstract) -->
  </foaf:Project>

  <!-- EPOS WebService -->

  <eposap:WebService rdf:about="[service uri]">
    <dct:title xml:lang="en">GFZ Data Services</dct:title> <!-- title -->
    <!-- abstract -->
    <dct:description xml:lang="en">Since 2004, the GFZ German Research Centre for Geosciences assigns Digital Object Identifiers (DOI) to datasets. These datasets are archived by and published through GFZ Data Services and cover all geoscientific disciplines. They range from large dynamic datasets deriving from data intensive global monitoring networks with real-time data acquisition to the full suite of highly variable datasets collected by individual researchers or small teams. These highly variable data (‘long-tail data’) are small in size, but represent an important part of the total scientific output.</dct:description>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</dct:issued><!-- date of publication -->
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">20016-12-31T12:00:00</dct:modified><!-- date of revision -->
<!-- @andrea: in DCAT-AP, the service licence is about the use conditions of the service itself, not of its content -->
    <dct:license> <!-- Access and Use Restrictions -->
      <dct:LicenseDocument>
        <dct:type>
          <skos:Concept>
            <skos:prefLabel>Creative Commons for data, Open Source licences for software</skos:prefLabel>
          </skos:Concept>
        </dct:type>
      </dct:LicenseDocument>
    </dct:license>
    <foaf:page rdf:resource="http://dataservices.gfz-potsdam.de/igets/showshort.php?"/>
<!-- @andrea: dcat:accessURL should be used only for dataset distributions -->    
    <dcat:accessURL></dcat:accessURL> <!-- NOT USED. LEAVE IT BLANK -->
<!-- @andrea: You can use the relevant codelist maintained by the EU Publications office -->    
    <dct:format rdf:resource="http://publications.europa.eu/resource/authority/file-type/ZIP"/><!-- distribution format -->
<!--    
    <dct:format>
      <dct:MediaTypeOrExtent>zip</dct:MediaTypeOrExtent>
    </dct:format>
-->    
    <dct:rights><!-- public access limitations -->
      <dct:RightsStatement>open data</dct:RightsStatement>
    </dct:rights> 
    <dct:conformsTo rdf:resource="http://www.isotc211.org/2005/gmd"/> <!-- Metadata standard -->
    <dct:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dataservices.gfz-potsdam.de</dct:identifier> <!-- Unique Resource Identifier -->
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</dct:created> <!-- date of creation -->
<!-- @andrea: You can use the relevant codelist maintained by the INSPIRE Registry -->    
    <dct:type> <!--RESOURCE TYPE This property refers to the type of service(access to data, access to metadata) -->
      <skos:Concept>
        <skos:prefLabel>metadata access</skos:prefLabel>
      </skos:Concept>
    </dct:type>
<!-- @andrea: You can use the relevant codelist maintained by the INSPIRE Registry -->    
    <dct:type rdf:resource=""/> <!-- This property refers to data service type (E.g. Discovery, Download, View..) -->
<!--        
    <dct:type>
      <skos:Concept>
        <skos:prefLabel>Discovery</skos:prefLabel>
      </skos:Concept>
    </dct:type>
-->    
    <dcat:keyword xml:lang="en">Laboratories, rocks</dcat:keyword> <!-- Keywords -->
    <dcat:theme> <!-- Domain -->
      <skos:Concept>
        <skos:prefLabel>Laboratories</skos:prefLabel>
      </skos:Concept>
    </dcat:theme>

    <!-- Parameters -->
<!-- @andrea: TBD -->
<!--
    <http:Parameter>
      <http:paramName>id</http:paramName>
      <http:paramValue>any value</http:paramValue>
      <dct:type>
        <skos:Concept>
          <skos:prefLabel>string</skos:prefLabel>
        </skos:Concept>
      </dct:type>
    </http:Parameter>    
-->    
    <eposap:vocabularyTerm>station,network</eposap:vocabularyTerm> <!-- vocabulary terms  -->
    <owl:versionInfo>1.0</owl:versionInfo> <!-- version of parameter -->
    <!-- - - - - -->

    <dcat:contactPoint rdf:resource="[http://.../]personID01"/> <!-- link to contact point (vcard:Individual) -->
    <eposap:responsibleParty rdf:resource="[http://.../]personID01"/> <!-- link to responsibleParty (vcard:Individual) -->
    <dct:spatial> <!-- Geographic location/spatial extent /bounding box -->
      <dct:Location>
        <locn:geometry rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">POLYGON(-10.58 70.09,34.59 70.09,34.59 34.56,-10.58 34.56, -10.58 70.09)</locn:geometry>
      </dct:Location>
    </dct:spatial>
<!-- @andrea: You can use the relevant codelist maintained by the INSPIRE Registry -->    
    <adms:representationTechnique rdf:resource=""/> <!-- Spatial representation type -->
<!--    
    <adms:representationTechnique>
      <skos:Concept>
        <skos:prefLabel>Vector</skos:prefLabel>
      </skos:Concept>
    </adms:representationTechnique>
-->    
    <dct:temporal> <!-- temporal extent -->
      <dct:PeriodOfTime>
        <schema:startDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2001-12-31T12:00:00</schema:startDate>
        <schema:endDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2016-12-31T12:00:00</schema:endDate>
      </dct:PeriodOfTime>
    </dct:temporal>
    <dct:provenance> <!-- Lineage -->
      <dct:ProvenanceStatement>
        <rdfs:label xml:lang="en">????????</rdfs:label>
      </dct:ProvenanceStatement>
    </dct:provenance>
<!-- @andrea: coupled resources are modelled with dct:hasPart in GeoDCAT-AP -->  
    <dct:hasPart rdf:resource="[coupled resource uri]"/> <!-- Coupled Resource -->
  </eposap:WebService>
  
</rdf:RDF>
