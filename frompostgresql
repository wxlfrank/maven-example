dataset (uuiddataset varchar, title varchar,abstract text, uri varchar)
datasetorganisation (uuiddataset varchar, uuidorgunit varchar, role text)
datasetperson (uuiddataset varchar, uuidpers varchar, role text)  
datasetlocation (uuiddataset varchar, minlat double precision,  maxlat double precision, minlon double precision,maxlon double precision, minelev double precision,minaxelev double precision)
datasetdates (uuiddataset varchar,  creation timestamp,  revision timestamp, publication timestamp)
datasetresolution (uuiddataset varchar,  resolution varchar)   
datasetspatialrepresentationtype (uuiddataset varchar,  type varchar)   
datasetspatialreferencesystem (uuiddataset varchar,  referencesystem varchar)   
datasetisotopic (uuiddataset varchar,  topic varchar)   
datasetformat (uuiddataset varchar,  format varchar)   
datasetkeyword (uuiddataset varchar,  keyword varchar)  
datasetthesauruskeyword (uuiddataset varchar,  keyword varchar, thesaurus varchar)   
datasetaccessanduserestriction (uuiddataset varchar,  accessanduse varchar)   
datasetpublicaccesslimit (uuiddataset varchar,  publicaccesslimit varchar)   
datasetlineage (uuiddataset varchar,  lineage varchar)   
datasettemporalextent (uuiddataset varchar,  startdate timestamp, enddate timestamp)   
datasetmetadata (uuiddataset varchar, uuidmeta varchar)
isometa (uuidmeta varchar, fileidentifier varchar, language varchar, charset varchar )  
isometadates (uuidmeta varchar, created timestamp, updated timestamp)
isometadetails (uuidmeta varchar, standardname varchar, standardversion varchar)
 
person (uuidpers varchar, firstname varchar,familyname varchar)
personfullname (uuidpers varchar, fullname varchar)
 
