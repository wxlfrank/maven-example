		<eposap:parameter>
			<http:paramName>id</http:paramName>
			<dct:type> <!-- This property refers to parameter type -->
				integer
			</dct:type>			
			<owl:versionInfo>1.0</owl:versionInfo> <!-- version of parameter -->
		</eposap:parameter>
		<eposap:parameter>
			<http:paramName>stationType</http:paramName>
			<dct:type> <!-- This property refers to parameter type -->
				string
			</dct:type>	
			<http:paramValue>mobile,static</http:paramValue>	<!-- allowed vocabulary terms, optional tag -->				
			<owl:versionInfo>1.5</owl:versionInfo> <!-- version of parameter -->
		</eposap:parameter>		
		<dcat:contactPoint>personID01</dcat:contactPoint> <!-- internal link to contact point (vcard:Individual) described in the same file in <eposap:Person> -->
		<eposap:responsibleParty>personID01</eposap:responsibleParty> <!-- internal link to responsibleParty (vcard:Individual) described in the same file in <eposap:Person> -->
		<dct:spatial> <!-- Geographic location/spatial extent /bounding box -->
			<dct:Location>
				<locn:geometry>POLYGON(-10.58 70.09,34.59 70.09,34.59 34.56,-10.58 34.56, -10.58 70.09)</locn:geometry>
			</dct:Location>
		</dct:spatial>
		<adms:representationTechnique> <!-- Spatial representation type -->
			Vector
		</adms:representationTechnique>
		<dct:temporal> <!-- temporal extent -->
			<dct:PeriodOfTime>
				<schema:startDate>2001-12-31T12:00:00</schema:startDate>
				<schema:endDate>2016-12-31T12:00:00</schema:endDate>
			</dct:PeriodOfTime>
		</dct:temporal>
		<dct:provenance> <!-- Lineage -->
